@model IEnumerable<BitClassroom.DAL.Models.Question>

@{
    ViewBag.Title = "Index";
}

<h2>Question</h2>

<p id="QuestionsIndexLinks">
    @Html.ActionLink("Create New Question", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.QContent)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.QContent)
        </td>
       
    </tr>
}

</table>


@using (Html.BeginForm("Index", "Home", "Home"))
{
    <input type="submit" class="btn btn-primary" value="Back" />
}